          <% end %>

          <%= link_to "https://www.vagrantup.com/", target: "_blank" do %>
            <%= image_tag "tecnologias/infra-vagrant.png", height: "40px", class: "mr-5" %>
          <% end %>

          <%= link_to "https://aws.amazon.com", target: "_blank" do %>
            <%= image_tag "tecnologias/infra-aws1.png", height: "45px" %>
          <% end %>
