      <div class="category">
        <div class="title">Infraestrutura</div>

        <div class="images d-flex align-items-center pt-4">
          <%= link_to "https://www.docker.com/", target: "_blank" do %>
            <%= image_tag "tecnologias/infra-docker.png", height: "60px", class: "mr-5" %>
